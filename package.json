{
  "name": "devnest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm format:check && pnpm lint && pnpm type-check",
    "steiger": "steiger ./src",
    "steiger:watch": "steiger ./src --watch",
    "supabase:gen-types": "bash -c 'export $(cat .env.local | grep SUPABASE_ACCESS_TOKEN | xargs) && supabase gen types typescript --project-id fadvyjklohgyyntezngk > src/shared/lib/supabase/types.ts'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.5.0",
    "highlight.js": "^11.11.1",
    "next": "15.5.4",
    "pretendard": "^1.3.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "zod": "^4.1.12"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@rushstack/eslint-config": "^4.4.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.1.25",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "steiger": "^0.5.11",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "browserslist": "> 1%"
}
